{
  "manifest_version": 3,
  "name": "TTAM GMV MAX Table Enhancer",
  "version": "1.0",
  "description": "增强TTAM GMV MAX 表格体验",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.tiktok.com/*",
        "*://*.bytedance.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}